{
    "app": {
        "name": "Operations Service",
        "version": "1"
    },
    "consul": {
        "enabled": true,
        "url": "http://ubuntu-docker-host:8500",
        "address": "ubuntu-docker-host",
        "pingEndpoint": ""
    },
    "fabio": {
        "enabled": true,
        "url": "http://ubuntu-docker-host:9999"
    },
    "httpClient": {
        "type": "fabio",
        "retries": 3,
        "services": {}
    },
    "logger": {
        "level": "trace",
        "console": {
            "enabled": true
        },
        "elk": {
            "enabled": false,
            "url": "http://ubuntu-docker-host:9200"
        },
        "file": {
            "enabled": false,
            "path": "logs/logs.txt",
            "interval": "day"
        },
        "seq": {
            "enabled": true,
            "url": "http://ubuntu-docker-host:5341",
            "apiKey": "secret"
        },
        "MinimumLevelOverrides": {
          
        }
    },
    "jaeger": {
        "enabled": true,
        "serviceName": "operations",
        "udpHost": "ubuntu-docker-host",
        "udpPort": 6831,
        "maxPacketSize": 0,
        "sampler": "const",
        "excludePaths": ["/", "/ping", "/metrics"]
    },
    "jwt": {
        "certificate": {
            "location": "",
            "password": "",
            "rawData": ""
        },
        "issuerSigningKey": "eiquief5phee9pazo0Faegaez9gohThailiur5woy2befiech1oarai4aiLi6ahVecah3ie9Aiz6Peij"
    },
    "metrics": {
        "enabled": false,
        "prometheusEnabled": false,
        "influxUrl": "http://ubuntu-docker-host:8086"
    },
    "mongo": {
        "connectionString": "mongodb://ubuntu-docker-host:27017"
    },
    "rabbitMq": {
        "hostnames": [
            "ubuntu-docker-host"
        ]
    },
    "redis": {
        "connectionString": "ubuntu-docker-host",
        "instance": "operations:"
    },
    "requests": {
        "expirySeconds": 300
    },
    "signalR": {
        "backplane": "redis"
    },
    "swagger": {
        "enabled": true,
        "reDocEnabled": false,
        "name": "v1",
        "title": "API",
        "version": "v1",
        "routePrefix": "docs",
        "includeSecurity": true
    },
    "vault": {
        "enabled": false,
        "url": "http://ubuntu-docker-host:8200",
        "kv": {
            "enabled": false
        },
        "pki": {
            "enabled": false
        },
        "lease": {
            "mongo": {
                "enabled": false,
                "templates": {
                    "connectionString": "mongodb://{{username}}:{{password}}@ubuntu-docker-host:27017"
                }
            }
        }
    }
}